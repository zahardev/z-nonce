  "type": "library",
  "keywords": [
    "nonce",
    "wordpress",
    "wp"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Sergey Zaharchenko",
      "email": "zaharchenko.dev@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "brain/monkey": "^1.4",
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.8|~5.1"
  },
  "autoload": {
    "psr-4": {
      "Zdev\\ZNonce\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zdev\\ZNonce\\Tests\\": "tests"
    }
  }
